  "keywords": [
    "php"
  ],
  "homepage": "https://github.com/msbios/msbios-json",
  "minimum-stability": "stable",
  "authors": [
    {
      "name": "Judzhin Miles",
      "email": "info@msbios.com"
    }
  ],
  "autoload": {
    "psr-4": {
